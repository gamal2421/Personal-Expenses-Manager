<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="3.1">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <session-config>
        <cookie-config>
            <secure>true</secure>
        </cookie-config>
    </session-config>

    <!-- Servlet mapping for AiAnalysisServlet -->
    <servlet>
        <servlet-name>AiAnalysisServlet</servlet-name>
        <servlet-class>javawork.personalexp.servlets.AiAnalysisServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AiAnalysisServlet</servlet-name>
        <url-pattern>/ai_suggests</url-pattern>
    </servlet-mapping>

</web-app>